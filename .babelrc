{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    ["@babel/preset-typescript", { "allowNamespaces": true, "allowDeclareFields": true, "onlyRemoveTypeImports": true }],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ]
}
